#: dashboard/__dashboard.php:9 dashboard/dashboard.php:9
msgid "Dashboard"
msgstr "Tableau de bord"

#: dashboard/__dashboard.php:10 dashboard/dashboard.php:10
msgid "Wizard"
msgstr "Magicien"

